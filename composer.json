{
    "name": "wallee/sdk",
    "version": "5.1.0",
    "description": "wallee SDK for PHP",
    "keywords": [
        "wallee",
        "payment",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/wallee-payment/php-sdk",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "wallee AG",
            "email": "info@wallee.com",
            "homepage": "https://www.wallee.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "firebase/php-jwt": "^6.8",
        "guzzlehttp/guzzle": "^7.7",
        "guzzlehttp/psr7": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.20"
    },
    "autoload": {
        "psr-4": { "Wallee\\Sdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Wallee\\Sdk\\Test\\" : "test/" }
    }
}
